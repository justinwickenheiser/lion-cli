{
    "name": "justinwickenheiser/lion-cli",
    "description": "A CLI to quickly spin up boiler-plate packages for gvsu-webteam development.",
    "keywords": ["php", "laravel", "lion", "cli", "gvsu"],
    "homepage": "https://github.com/justinwickenheiser/lion-cli",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/justinwickenheiser/lion-cli/issues",
        "source": "https://github.com/justinwickenheiser/lion-cli"
    },
    "authors": [
        {
            "name": "Justin Wickenheiser"
        }
    ],
    "require": {
        "illuminate/database": "^10.5",
        "php": "^8.1",
        "nunomaduro/termwind": "^1.15"
    },
    "require-dev": {
        "laravel/pint": "^1.5",
        "laravel-zero/framework": "^10.0",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^1.22.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["builds/lion"]
}
